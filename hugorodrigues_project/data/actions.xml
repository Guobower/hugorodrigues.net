<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_project" model="ir.actions.server">
        <field name="name">Hugo Rodrigues Project</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id"
            ref="hugorodrigues_project.model_hugo_rodrigues_project" />
        <field name="state">code</field>
        <field name="code">
model.doconfig()
        </field>
    </record>

    <record id="action_project_todo" model="ir.actions.todo">
        <field name="action_id" ref="action_project" />
        <field name="state">open</field>
    </record>
</odoo>
