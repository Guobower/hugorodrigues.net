<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_install" model="ir.actions.server">
        <field name="name">Hugo Rodrigues Blog</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id"
            ref="hugorodrigues_blog.model_hugo_rodrigues_blog" />
        <field name="state">code</field>
        <field name="code">
model.doconfig()
        </field>
    </record>

    <record id="action_install_todo" model="ir.actions.todo">
        <field name="action_id" ref="action_install" />
        <field name="state">open</field>
    </record>
</odoo>
